{
  "name": "node-http",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig-ssr.json tsc-alias -p tsconfig-ssr.json --outDir dist --declarationDir dist",
    "build:test": "rm -rf dist && tsc --project tsconfig-ssr.json && tsc-alias -p tsconfig-ssr.json",
    "watch": "nodemon -e ts -w ./src --exec 'ts-node --project tsconfig-ssr.json -r tsconfig-paths/register src/server.ts'",
    "build:parcel": "rm -rf ./public && parcel build src/index.html --out-dir ./public",
    "serve:parcel": "rm -rf ./public && parcel --port 3000 src/index.html --out-dir ./public",
    "watch:parcel": "rm -rf ./public && parcel watch src/index.html --out-dir ./public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "alias": {
    "@fetch": "./node_modules/simple-boot-http-ssr/fetch/front"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.12",
    "parcel-bundler": "^1.12.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.3.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "simple-boot-http-ssr": "^1.0.2"
  }
}
